@model BookishWebApplication.Models.View.BooksViewModel
@{
    ViewData["Title"] = "Books";
}

<div class="text-center">
    <h1 class="display-4">Books</h1>
    <p>yeah books.</p>
    
    @{
        foreach (var book in Model.Books)
        {
            <p>Book: @book.Title</p>
            <p>Published: @book.PublicationYear</p>
            <br/>
        }
    }
    
</div>